<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="768.0" prefWidth="1366.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.naval_battle.MenuController">
    <stylesheets>
        <URL value="@/styles/menu.css" />
    </stylesheets>
   <children>
      <ImageView fitHeight="782.0" fitWidth="1374.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/menu.png" />
         </image>
      </ImageView>
      <Button fx:id="register_btn" layoutX="621.0" layoutY="459.0" mnemonicParsing="false" onAction="#goToRegister" prefHeight="32.0" prefWidth="132.0" text="Register" />
      <Button fx:id="tutorial_btn" layoutX="621.0" layoutY="507.0" mnemonicParsing="false" onAction="#goToTutorial" prefHeight="32.0" prefWidth="132.0" text="How to play" />
      <Button fx:id="credits_btn" layoutX="621.0" layoutY="553.0" mnemonicParsing="false" onAction="#goToCredits" prefHeight="32.0" prefWidth="132.0" text="Credits" />
      <Button fx:id="quit_btn" layoutX="621.0" layoutY="600.0" mnemonicParsing="false" onAction="#quitGame" prefHeight="32.0" prefWidth="132.0" text="Quit" />
   </children>
</AnchorPane>
